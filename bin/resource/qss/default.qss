/*-------------------------DlgLogin------------------------------------*/
QDialog#DlgLogin{
border-image:url(:/image/background_login.png);
}

QDialog#DlgLogin QLabel#label_title {
font-size: 30px;
font-family: "Microsoft YaHei";

spacing:50px;
