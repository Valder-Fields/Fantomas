

/*-------------------------QWidget------------------------------------*/
QWidget{

font-size: 16px;
font-family: "Microsoft YaHei";
font-weight: normal;
background: #ffffff;
color:#090909;
}

/*-------------------------QMainWindow------------------------------------*/
QMainWindow
{
background:#15304c;
/*background:#e2edf2;*/
border-top:0px;
border-bottom:0px;
}

QWidget#centralwidget{
border-left: 5px solid #e2edf2;
border-right: 5px solid #e2edf2;
border-top: 0px solid #e2edf2;
border-bottom: 10px solid #e2edf2;
}

/*-------------------------QScrollArea------------------------------------*/
QScrollArea{
border: 0px solid;
}

/*-------------------------QTabWidget------------------------------------*/
QTabWidget::pane { /* The tab widget frame */
top:0px;
background:#f7fafa;
    border: 1px solid #a7a7a7;
}

QTabWidget QTabBar::tab {
margin:0px 0px -1px 0px;
    border:1px solid #a7a7a7;
    min-width: 200px;
    max-width: 200px;
    min-height: 38px;
    max-height: 38px;
    
    background:#e2edf2;
    color:#090909;
}

QTabWidget QTabBar::tab:selected {
    background:#7890a5;
    color:#ffffff;
}

/*-------------------------QPushButton------------------------------------*/
QPushButton{
border:0px solid;
background:#237ad2;
color:#ffffff;
width:100px;
height:30px;
border-radius: 3px;
}

QPushButton::hover{
background:#30669d;
}

QPushButton::pressed{
background:#237ad2;
}


/*-------------------------QLineEdit------------------------------------*/
QLineEdit:disabled {
background:#f0f0f0;
color:#787878;
min-height:25px;
max-height:25px;
}


/*-------------------------QTextEdit------------------------------------*/
QTextEdit:disabled {
background:#f0f0f0;
}

/*-------------------------QCheckBox------------------------------------*/
QCheckBox::indicator:unchecked {
padding: 3px 0px 0px 0px;
   image: url(:/image/qss/checkbox_unchecked.png);
}

QCheckBox::indicator:checked {
padding: 3px 0px 0px 0px;
   image: url(:/image/qss/checkbox_checked.png);
}

/*-------------------------QDialog------------------------------------*/


/*-------------------------QMessageBox------------------------------------*/

/*-------------------------QMenu------------------------------------*/
QMenu
{	
font-size: 16px;
font-family: "Microsoft YaHei";
background: #e2edf2;
border: 1px solid #778fa4;
}

QMenu::item
{
margin-left: 0px;
padding: 5px 25px 5px 20px;
}

QMenu:pressed {
background: #c1d0de;
}

QMenu::item:selected {
background: #c1d0de;
}

/*-------------------------QMenuBar------------------------------------*/

QMenuBar {
font-size: 18px;
background: #e2edf2;
color: #1b1b1b;
min-height:25px;
max-height:25px;
padding-top:5px;
padding-right:0px;
padding-bottom:10px;
padding-left:5px;
}
QMenuBar::item {
	padding:2px 15px 2px 15px;
}
QMenuBar::item:selected {
background: #b0d6fd;
}
QMenuBar::item:pressed {
background: #c1d0de;
} 

/*-------------------------QToolBar------------------------------------*/
QToolBar{
background:#e2edf2;
min-height:68px;
padding:6px 0px 6px 5px;
spacing:6px;
border-top:1px solid #798fa5;
}

QToolBar QToolButton{
font-family:"Microsoft YaHei";
font-size:18px;
width:77px;
height:65px;

padding-top:0px;
padding-bottom:0px;
padding-left:5px;
padding-right:5px;
border-color:black;
background-color: transparent;
}

QToolBar QToolButton:disable{
padding-left:5px;
padding-right:5px;
}

QToolBar QToolButton:hover{
border-radius: 3px;
background-color: #b0d6fd;
}

QToolBar QToolButton:pressed{
border-radius: 3px;
background-color: #c1d0de;
}

QToolBar QToolButton:checked{
border-radius: 3px;
background-color: #c1d0de;
}

/*-------------------------QTreeWidget------------------------------------*/

QTreeWidget QHeaderView::section {
font-family:"Microsoft YaHei";
font-size:16px;
font-weight:bold;
color: #1b1b1b;

background-color: #ecf1f5;
padding-left: 4px;
border: 0px solid;
}

QTreeWidget QHeaderView::section {
min-height:34px;
}

QTreeWidget {
font-family:"Microsoft YaHei";
font-size:16px;
color: #1b1b1b;
outline:none;

border: 1px solid;
border-color:#a7a7a7;
}

QTreeWidget::item {
height:30px;
border: 0px solid;
}

QTreeWidget::item:selected {
background:#ededed;
color: #1b1b1b;
}

/*-------------------------QGroupBox------------------------------------*/
QGroupBox {
font-size: 16px;
font-family: "Microsoft YaHei";
font-weight:bold;
color:#1b1b1b;

border:1px solid #a7a7a7;
margin-top: 11px;
}

QGroupBox::title {
subcontrol-origin: margin;
left:20px;
padding: 0px 10px 0px 10px;
}


/*-------------------------QRadioButton------------------------------------*/
QRadioButton {
font-size: 16px;
font-family: "Microsoft YaHei";
color:#090909;
}

QRadioButton::indicator {
    margin-top:3px;
}

QRadioButton::indicator::unchecked::disabled {
	 image: url(:/image/qss/radiobtn_unchecked_disabled.png);
}

QRadioButton::indicator::checked::disabled {
	 image: url(:/image/qss/radiobtn_checked_disabled.png);
}

QRadioButton:disabled {
color:#b2b2b2;
}

QRadioButton::indicator::unchecked {
    image: url(:/image/qss/radiobtn_unchecked.png);
}

QRadioButton::indicator:unchecked:hover {
    image: url(:/image/qss/radiobtn_unchecked.png);
}

QRadioButton::indicator:unchecked:pressed {
    image: url(:/image/qss/radiobtn_unchecked.png);
}

QRadioButton::indicator::checked {
    image: url(:/image/qss/radiobtn_checked.png);
}

QRadioButton::indicator:checked:hover {
    image: url(:/image/qss/radiobtn_checked.png);
}

QRadioButton::indicator:checked:pressed {
    image: url(:/image/qss/radiobtn_checked.png);
}

/*-------------------------QScrollBar------------------------------------*/
QScrollBar:horizontal {
background: #e2edf2;
height: 22px;
margin: 0px 20px 0 20px;
}

QScrollBar::handle:horizontal {
background: #c1d0de;
min-width: 20px;
}

QScrollBar::add-line:horizontal {    
background: #e2edf2;
width: 22px;
subcontrol-position: right;
subcontrol-origin: margin;
}

QScrollBar::sub-line:horizontal {
background: #e2edf2;
width: 22px;
subcontrol-position: left;
subcontrol-origin: margin;
}

QScrollBar:left-arrow:horizontal{
width: 20px;
height: 20px;
image: url(:/image/qss/scrollbar_left.png);
}

QScrollBar::right-arrow:horizontal {
width: 20px;
height: 20px;
image: url(:/image/qss/scrollbar_right.png);
}

QScrollBar:vertical {
background: #e2edf2;
width: 22px; /* == QComboBox::down-arror width*/
margin: 20px 0 20px 0;
}

QScrollBar::handle:vertical {
background: #c1d0de;
min-height: 20px;
}

QScrollBar::add-line:vertical { 
background: #e2edf2;
height: 20px;
subcontrol-position: bottom;
subcontrol-origin: margin;
}

QScrollBar::sub-line:vertical { 
background: #e2edf2;
height: 20px;
subcontrol-position: top;
subcontrol-origin: margin;
}

QScrollBar:up-arrow:vertical{
width: 20px;
height: 20px;
image: url(:/image/qss/scrollbar_up.png);
}

QScrollBar::down-arrow:vertical {
width: 20px;
height: 20px;
image: url(:/image/qss/scrollbar_down.png);
}

/*-------------------------QComboBox------------------------------------*/
QComboBox {
font-size: 16px;
font-family: "Microsoft YaHei";
color:#090909;

min-height:25px;
max-height:25px;

border: 1px solid #a7a7a7;
}

QComboBox:disabled {
color:#b2b2b2;
}

QComboBox:hover {
background: #e2edf2;
}

QComboBox::down-arrow {
image: url(:/image/qss/combobox_arrow.png);
}

QComboBox::down-arrow:on { /* shift the arrow when popup is open */
top: 1px;
left: 1px;
}

QComboBox::drop-down {
subcontrol-origin: padding;
subcontrol-position: top right;
width: 22px;

border-left-width: 1px;
border-left-color: #a7a7a7;
border-left-style: solid; /* just a single line */
}

QComboBox QAbstractItemView{
selection-background-color:#e2edf2;
selection-color:#090909;
}

/*-------------------------QDateTimeEdit------------------------------------*/
QDateEdit {
border: 1px solid #a7a7a7;

font-family: "微软雅黑";
min-height: 25px;
}

QDateEdit::drop-down {

subcontrol-origin: padding;
subcontrol-position: top right;
width: 22px;

border-left-width: 1px;
border-left-color: #a7a7a7;
border-left-style: solid; /* just a single line */
border-top-right-radius: 3px; 
border-bottom-right-radius: 3px;
}

